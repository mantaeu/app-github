{
  "name": "mantaevert-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "if not exist public mkdir public && if exist web-build (xcopy web-build\\* public\\ /E /Y) else (echo web-build not found, creating placeholder && echo ^<h1^>App Building...^</h1^> > public\\index.html)",
    "build": "cd backend && npm install && npm run build",
     "backend": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "predeploy": "npm run web:build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.0.0",
    "expo": "~53.0.0",
    "expo-constants": "~17.1.7",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-image-manipulator": "~13.1.7",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-print": "~14.1.4",
    "expo-router": "~5.1.3",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "i18n-js": "^4.4.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~19.0.10",
    "gh-pages": "^6.3.0",
    "typescript": "^5.3.3"
  },
  "private": true
}
